{
  "presets": [
    "es2015",
    "stage-2",
    "react"
  ],
  "plugins": [
    "transform-class-properties",
    "syntax-dynamic-import",
    ["babel-plugin-root-import", [{
      "rootPathPrefix": "~",
      "rootPathSuffix": "./"
    }, {
      "rootPathPrefix": "#",
      "rootPathSuffix": "./resources/assets/js/"
    }]]
  ]
}
